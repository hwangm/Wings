body
  padding: 50px
  font: 14px sans-serif
  -webkit-font-smoothing: antialiased
a
  color: #00B7FF
#step3 select
  margin: 10px 10px 10px 10px
  width: 150px
#buttonContainer button
  margin:auto 5px auto 0
  width:100%
#stepCounter
  margin:auto 5px auto 0
#navHeader div
  text-align: center
  color: white
  font-weight: bold
  font-size: 16px
#extraDetails
  margin-top: 6px
  color: white
.nested-group input:focus
  box-shadow: none
.nested-group div:first-child
  border-bottom-right-radius: 0
  border-top-right-radius: 0
  border-top-left-radius: 4px
  border-bottom-left-radius: 4px
  border-right: 1px solid #E5E5E5
.nested-group input
  margin-left: -5px
  border: 0 solid white
  border-style: solid
  background-color: white
  box-shadow: inset 0 0 0 0 red
  padding: 10px 16px 10px 5px
.nested-group button
  background-color: black
  color: white
  border-color: white
  padding: 21px 12px 21px 12px
  width: 100px
  font-size: 16px
  font-weight: bold
.nested-group .inline-button
  display: inline-block
  padding-top: 11px
  padding-bottom: 11px
  padding-right: 11px
  background-color: white
  margin-bottom: 0
  border-left-color: white
  border-left-width: 0
  margin-left: -1px
  height: auto
  border-top-right-radius: 4px
  border-bottom-right-radius: 4px
.nested-group div
  display: inline-block
  background-color: white
  padding: 12px 15px 5px 15px
.nested-group label
  display: block
  color: #191C23
  font-weight: normal
  font-size: 12px
  line-height: 16px
.inline-toggle
  background-color: transparent
  color: white
.more-options
  color: grey
body > div.container > h3
  color: white
  font-size: 28px
  line-height: 36px
  font-weight: bold
  margin-bottom: 12px
  margin-top: 48px
#extraDetails > div > div > button
  color: white
  background-color: transparent
  padding-top: 4px
  padding-left: 6px
  padding-right: 20px
span.filter-option.pull-left
  font-weight:700
