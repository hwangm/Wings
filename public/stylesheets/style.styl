body
  padding: 50px
  font: 14px sans-serif
  -webkit-font-smoothing: antialiased
a
  color: #00B7FF
#step3 select
  margin: 10px 10px 10px 10px
  width: 150px
#buttonContainer button
  margin:auto 5px auto 0
  width:100%
#stepCounter
  margin:auto 5px auto 0
#navHeader div
  text-align: center
  color: white
  font-weight: bold
  font-size: 16px
#extraDetails
  margin-top: 6px
  color: white
.nested-group input:focus
  box-shadow: none
.nested-group div:first-child
  border-bottom-right-radius: 0
  border-top-right-radius: 0
  border-top-left-radius: 4px
  border-bottom-left-radius: 4px
  border-right: 1px solid #E5E5E5
.nested-group input
  margin-left: -5px
  border: 0 solid white
  border-style: solid
  background-color: white
  box-shadow: inset 0 0 0 0 red
  padding: 0 16px 8px 5px
.nested-group button
  background-color: #0F2332
  color: white
  padding: 21px 24px 21px 24px
  width: auto
  font-weight: bold
.nested-group .inline-button
  display: inline-block
  padding-top: 9px
  padding-bottom: 9px
  padding-right: 9px
  background-color: white
  margin-bottom: 0
  border-left-color: white
  border-left-width: 0
  margin-left: -1px
  height: auto
  border-top-right-radius: 4px
  border-bottom-right-radius: 4px
.nested-group div
  display: inline-block
  background-color: white
  padding: 12px 15px 2px 15px
  color: #191C23 !important
.nested-group label
  display: block
  font-weight: normal
  margin-bottom: 2px
.pac-container
  -webkit-box-shadow: none
  -moz-box-shadow: none
  box-shadow: none
.pac-item-query
  font-size: 14px
.pac-item
  font-size: 14px
  font-family: sans-serif
  padding: 16px 8px
  line-height: 18px
pac-item:hover
  background-color: #0F2332
  color: white
.pac-logo::after
  display: none !important
.pac-icon
  display: none !important
.inline-toggle
  background-color: transparent
  color: white
.more-options
  color: grey
body > div.container > h3
  color: white
  font-size: 28px
  line-height: 36px
  font-weight: bold
  margin-bottom: 16px
  margin-top: 48px
#extraDetails > div > div > button
  color: white
  background-color: transparent
  padding-top: 4px
  padding-left: 2px
  padding-right: 2px
span.filter-option.pull-left
  font-weight:700
  width: auto !important
  padding-right: 2px
.ui-menu .ui-menu-item
  font-size: 14px
#extraDetails > div > div
  padding-left: 2px
  padding-right: 2px
::-webkit-input-placeholder
  color: #C2C5CC
:-moz-placeholder
  color: #C2C5CC
::-moz-placeholder
  color: #C2C5CC
  opacity: 1
:-ms-input-placeholder
  color: #C2C5CC
